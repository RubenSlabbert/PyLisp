(library (square)
        (define (square-area l w)
            (* l w))
        
        (define (square-perimeter l w)
            (+ (* 2 l) (* 2 w))))

(import square)

(define area-test (= (square-area 2 2) 4))
(define perimeter-test (= (square-perimeter 2 2) 8))

(if 
        (and 
            (= area-test #t)
            (= perimeter-test #t))
        (print "All tests passed")
        (print "Test(s) failed"))

(let 
  ((f (lambda (x) (+ x x))))
  (print (f 2)))

(print ((lambda (x) (+ x x)) 2))
